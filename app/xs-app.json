{
    "welcomeFile": "index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/catalog/(.*)$",
            "authenticationType": "xsuaa",
            "scope": "$XSAPPNAME.Viewer",
            "destination": "srv"
        }
        ,
        {
            "source": "^/(.*)$",
            "authenticationType": "xsuaa",
            "scope": "$XSAPPNAME.Viewer",
            "localDir": "resources"
        },
        {
            "source": "^/user-api(.*)",
            "target": "$1",
            "service": "sap-approuter-userapi"
        }
    ]
}